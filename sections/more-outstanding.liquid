 <style>
  .container-outstanding {
  width: 95%;
  max-width: 1600px;
  margin: 0 auto;
  padding: 0 16px;
}

.container-outstanding div {
  width: 100%;
}
.container-image-outstanding {
  width: 100%;
  margin-top: 50px;
  display: grid;
  grid-template-columns: repeat(1, 1fr);
  gap: 20px;
}
.container-image-outstanding div {
  width: 100%;
  height: auto;
  cursor: pointer;
  transition: all 0.5s ease-in-out;
  overflow: hidden;
}
.container-image-outstanding div img {
  width: 100%;
  height: auto;

  transition: all 0.5s ease-in-out;
}
.container-image-outstanding div img:hover {
  transform: scale(1.1);
}

@media screen and (min-width: 768px) {
  .container-image-outstanding {
    grid-template-columns: repeat(2, 1fr);
  }

}

</style> 

<div>
  
  {% render 'heading', 
        first-title: section.blocks[0].settings.text_1,
        second-title: section.blocks[1].settings.text_2,
        
      %}
      <p class="paragraph-section">{{section.blocks[2].settings.paragraph}}</p>

  <div class="container-image-outstanding">
    <div>
      <img  src="{{section.blocks[3].settings.image_1 | img_url: 'grande'}}" alt="">

    </div>
    <div>
      <img  src="{{section.blocks[4].settings.image_2 | img_url: 'grande'}}" alt="">

    </div>
  </div>
</div>

{% schema %}
  {
    "name": "outstanding",
    "tag": "section",
    "class": "container-outstanding",
    "limit": 1,
    "blocks": [
      {
        "name": "Title #1",
        "type": "title_1",
        "settings": [
          {
            "type": "text",
            "id": "text_1",
            "label": "Texto numero uno"
          }
        ]
      },
      {
        "name": "Title #2",
        "type": "title_2",
        "settings": [
          {
            "type": "text",
            "id": "text_2",
            "label": "Texto numero dos"
          }
        ]
      },
      {
        "name": "Paragraph",
        "type": "paragraph",
        "settings": [
          {
            "type": "text",
            "id": "paragraph",
            "label": "Parrafo"
          }
        ]
      },
      {
        "name": "Image #1",
        "type": "image_1",
        "settings": [
          {
            "type": "image_picker",
            "id": "image_1",
            "label": "Imagen uno"
          }
        ]
      },
      {
        "name": "Image #2",
        "type": "image_2",
        "settings": [
          {
            "type": "image_picker",
            "id": "image_2",
            "label": "Imagen dos"
          }
        ]
      }
    ]

  }
{% endschema %}
